
<!DOCTYPE html>
 <html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>SuperMarket Management</title>
        <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.css"/>
    </head>
    <body class="container">
        <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="#">SuperMarket Management</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-item nav-link active" asp-controller="home" asp-action="index">Home <span class="sr-only">(current)</span></a>
                    <a class="nav-item nav-link active" asp-controller="categories" asp-action="index">Categories <span class="sr-only">(current)</span></a>

                </div>
            </div>
        </nav>
        </header>
        <div>
        @RenderBody()
        </div>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
